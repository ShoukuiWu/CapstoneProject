@model IEnumerable<ConeInfoManagement.Models.Account>

@{
    ViewBag.Title = "Lists of Account Information";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.logonId)*@
            @Html.DisplayName("ID")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.passwd)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.logonName)*@
            @Html.DisplayName("Name")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.logonHours)*@
            @Html.DisplayName("Logon")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.logoffHours)*@
            @Html.DisplayName("Logoff")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.logonStatus)*@
            @Html.DisplayName("Staus")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.startDate)*@
            @Html.DisplayName("Start Date")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.endDate)*@
            @Html.DisplayName("End Date")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.roles)*@
            @Html.DisplayName("Role")
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Person.firstName)*@
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.logonId)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.passwd)*@
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logonName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logonHours)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logoffHours)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.logonStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.startDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.endDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roles)
        </td>
        <td>
            @*@Html.DisplayFor(modelItem => item.Person.firstName)*@
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
